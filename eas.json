{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "COSMOS_LINK_URL": "http://localhost:3000/api/v1/"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "COSMOS_LINK_URL": "https://cosmos-link.kiruse.dev/api/v1/"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "COSMOS_LINK_URL": "https://cosmos-link.kiruse.dev/api/v1/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
